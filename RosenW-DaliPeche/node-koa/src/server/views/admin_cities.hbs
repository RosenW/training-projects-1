<html lang="en">
  <head>
    <link rel="stylesheet" type="text/css" href="./../bootstrap.css">
  </head>
   <body>
    {{> adminForm }}
    <h2 class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">Credit Transfers</h2>
    <table id="users-table" class="table table-hover">
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Country Code</th>
        <th>Longitude</th>
        <th>Latitude</th>
        <th>Observed at</th>
      </tr>
      {{#each cities}}
        <tr class="gen-tr">
          <td>{{id}}</td>
          <td>{{name}}</td>
          <td>{{country_code}}</td>
          <td>{{lng}}</td>
          <td>{{lat}}</td>
          <td>{{observed_at}}</td>
        </tr>
      {{/each}}
    </table>
  </body>
</html>